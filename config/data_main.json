{
  "cli": {
    "description": "Multi-Agent Orchestrator fuer Data/ML Rollen."
  },
  "final_role_id": "data_integrator",
  "diff_safety": {
    "allowlist": [
      "config/data_main.json",
      "config/data_roles/*"
    ]
  },
  "roles": [
    {
      "id": "model_designer",
      "file": "data_roles/model_designer.json",
      "instances": 1,
      "depends_on": [],
      "expected_sections": [
        "# Model-Design",
        "- Ziel:",
        "- Features:",
        "- Modellwahl:",
        "- Risiken:",
        "- Plan (max 8 Schritte):"
      ],
      "timeout_sec": 1800,
      "max_prompt_chars": 16000
    },
    {
      "id": "data_engineer",
      "file": "data_roles/data_engineer.json",
      "instances": 1,
      "depends_on": [
        "model_designer"
      ],
      "expected_sections": [
        "# Data-Engineering",
        "- Pipelines/Quellen:",
        "- Transformationen:",
        "- Qualitaetschecks:",
        "- Offene Punkte:",
        "```diff"
      ],
      "apply_diff": true,
      "timeout_sec": 900
    },
    {
      "id": "evaluator",
      "file": "data_roles/evaluator.json",
      "instances": 1,
      "depends_on": [
        "data_engineer"
      ],
      "expected_sections": [
        "# Evaluation",
        "- Metriken:",
        "- Baselines:",
        "- Testset:",
        "- Offene Punkte:"
      ]
    },
    {
      "id": "ml_reviewer",
      "file": "data_roles/ml_reviewer.json",
      "instances": 1,
      "depends_on": [
        "evaluator"
      ],
      "expected_sections": [
        "# ML-Review",
        "- Findings:",
        "- Risiko/Impact:",
        "- Empfohlene Aenderungen:"
      ]
    },
    {
      "id": "data_integrator",
      "file": "data_roles/data_integrator.json",
      "instances": 1,
      "depends_on": [
        "model_designer",
        "data_engineer",
        "evaluator",
        "ml_reviewer"
      ],
      "expected_sections": [
        "# Final",
        "- Zusammenfassung:",
        "- Naechste Schritte:"
      ]
    }
  ]
}
