{
  "cli": {
    "description": "Multi-Agent Orchestrator fuer QA/Test Rollen."
  },
  "final_role_id": "qa_integrator",
  "diff_safety": {
    "allowlist": [
      "agent_families/qa_main.json",
      "agent_families/qa_agents/*"
    ]
  },
  "roles": [
    {
      "id": "test_strategist",
      "file": "qa_agents/test_strategist.json",
      "instances": 1,
      "depends_on": [],
      "expected_sections": [
        "# Teststrategie",
        "- Ziele:",
        "- Scope:",
        "- Testtypen:",
        "- Risiken:",
        "- Plan (max 8 Schritte):"
      ],
      "timeout_sec": 1800,
      "max_prompt_chars": 16000,
      "cli_provider": "gemini",
      "model": "gemini-2.5-flash",
      "cli_parameters": {
        "temperature": 0.5
      }
    },
    {
      "id": "test_author",
      "file": "qa_agents/test_author.json",
      "instances": 1,
      "depends_on": [
        "test_strategist"
      ],
      "expected_sections": [
        "# Tests (Autor)",
        "- Testfaelle:",
        "- Daten/Fixtures:",
        "- Abdeckungsluecken:",
        "- Offene Punkte:",
        "```diff"
      ],
      "apply_diff": true,
      "timeout_sec": 900,
      "cli_provider": "gemini",
      "model": "gemini-2.5-flash",
      "cli_parameters": {
        "temperature": 0.5
      }
    },
    {
      "id": "test_executor",
      "file": "qa_agents/test_executor.json",
      "instances": 1,
      "depends_on": [
        "test_author"
      ],
      "expected_sections": [
        "# Testausfuehrung",
        "- Auszufuehrende Kommandos:",
        "- Erwartete Ergebnisse:",
        "- Beobachtete Risiken:"
      ],
      "cli_provider": "gemini",
      "model": "gemini-2.5-flash",
      "cli_parameters": {
        "temperature": 0.5
      }
    },
    {
      "id": "bug_triager",
      "file": "qa_agents/bug_triager.json",
      "instances": 1,
      "depends_on": [
        "test_executor"
      ],
      "expected_sections": [
        "# Bug-Triage",
        "- Findings:",
        "- Prioritaet/Schwere:",
        "- Repro Schritte:",
        "- Empfohlene Fixes:"
      ],
      "cli_provider": "codex"
    },
    {
      "id": "qa_integrator",
      "file": "qa_agents/qa_integrator.json",
      "instances": 1,
      "depends_on": [
        "test_strategist",
        "test_author",
        "test_executor",
        "bug_triager"
      ],
      "expected_sections": [
        "# Final",
        "- Zusammenfassung:",
        "- Naechste Schritte:"
      ],
      "cli_provider": "claude",
      "model": "haiku",
      "cli_parameters": {
        "max_turns": 1
      }
    }
  ]
}