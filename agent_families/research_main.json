{
  "cli": {
    "description": "Multi-Agent Orchestrator fuer Research/UX Rollen."
  },
  "final_role_id": "research_integrator",
  "diff_safety": {
    "allowlist": [
      "agent_families/research_main.json",
      "agent_families/research_agents/*"
    ]
  },
  "roles": [
    {
      "id": "user_researcher",
      "file": "research_agents/user_researcher.json",
      "instances": 1,
      "depends_on": [],
      "expected_sections": [
        "# User Research",
        "- Ziele:",
        "- Methoden:",
        "- Findings:",
        "- Offene Punkte:"
      ],
      "timeout_sec": 1800,
      "max_prompt_chars": 16000,
      "cli_provider": "codex"
    },
    {
      "id": "ux_analyst",
      "file": "research_agents/ux_analyst.json",
      "instances": 1,
      "depends_on": [
        "user_researcher"
      ],
      "expected_sections": [
        "# UX-Analyse",
        "- Schmerzpunkte:",
        "- Hypothesen:",
        "- Prioritaeten:",
        "- Offene Punkte:"
      ],
      "cli_provider": "codex"
    },
    {
      "id": "a11y_reviewer",
      "file": "research_agents/a11y_reviewer.json",
      "instances": 1,
      "depends_on": [
        "ux_analyst"
      ],
      "expected_sections": [
        "# A11y-Review",
        "- Findings:",
        "- Impact:",
        "- Empfohlene Anpassungen:"
      ],
      "cli_provider": "claude",
      "model": "opus",
      "cli_parameters": {
        "max_turns": 2,
        "append_system_prompt": "Fokus: Security, Performance, Maintainability"
      }
    },
    {
      "id": "research_integrator",
      "file": "research_agents/research_integrator.json",
      "instances": 1,
      "depends_on": [
        "user_researcher",
        "ux_analyst",
        "a11y_reviewer"
      ],
      "expected_sections": [
        "# Final",
        "- Zusammenfassung:",
        "- Naechste Schritte:"
      ],
      "cli_provider": "claude",
      "model": "haiku",
      "cli_parameters": {
        "max_turns": 1
      }
    }
  ]
}